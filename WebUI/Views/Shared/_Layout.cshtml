<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/stylePreloader.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" href="">
    <link rel="stylesheet" href="~/Content/themes/base/jquery-ui.css">
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @Html.ActionLink("Map", "Map", "Map", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Address", "Index", "Address", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Street", "Index", "Street", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("Subdivision", "Index", "Subdivision", new { area = "" }, new { @class = "navbar-brand" })
                    @Html.ActionLink("About", "About", "About", new { area = "" }, new { @class = "navbar-brand" })
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <div id="hellopreloader">
        <div id="hellopreloader_preload"></div>
        <p>
            <a href="http://hello-site.ru"></a>
        </p>
    </div>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Preloader/preloaderScript.js"></script>
    @RenderSection("scripts", required: false)
    @RenderSection("DeleteAddressScript", required: false)
    @RenderSection("DeleteStreetScript", required: false)
    @RenderSection("DeleteSubdivisionScript", required: false)
    
</body>
</html>